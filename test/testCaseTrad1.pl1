programa cuatro:
  constantes
  
  	A = 10;
	B = 20;
  tipos
  	tvector = vector [1..3] de entero;
  
  
  variables
	a1:entero;
	b1:entero;
	
	resta : entero;
	mult  : entero;
	
	bol1 : booleano;
	bol2 : booleano;
	bol3 : booleano;
	
	v: tvector;
	
  comienzo

#(* se tiene que imprimir:*)
#(*Hola*)
#(*Mundo!*)

	escribir("Hola");
  	escribir("Mundo!");
    
#(* salida por pantalla resta de enteros*)
#(* se tiene que imprimir 5*)
	escribir(2+3);


  #(* pruebas con operaciones *)
#(* se tiene que imprimir 20*)



    a1=A;
    b1=2;

    mult=a1*b1;

    escribir(mult);
    
	bol1=cierto;
	bol2=falso;
	bol3=(bol1 y bol2);
	
# Tipos de datos. Arrays*)
# se tienen que imprimir las siguientes lineas:*)
#(*Pos 1:*)
#(*1*)
#(*Pos 3:*)
#(*4*)	
	
	v[1] = 1;
	v[2] = 2;
	v[3] = v[v[2]] * v[v[v[2]]];

	escribir("Pos 1:");
	escribir(v[1]);

	escribir("Pos 3:");
	escribir(v[3]);

#(* condiciona if*)
#(* se tienen que imprimir las siguientes loneas:*)
#(*resta (5):*)
#(*15*)

    a1=5;
    b1=10;
    
	si a1<b1 entonces:
		resta=a1+b1;
	sino:
		resta=20;
	fin si;    
    escribir("suma (5):");
    escribir(resta);
    
    #(* bucle for*)
#(* se tienen que imprimir las siguientes lineas:*)
#(*1*)
#(*2*)
#(*3*)
#(*4*)
#(*5*)
#(*6*)
#(*7*)
#(*8*)
#(*9*)
    
    
    para a1 en 1 .. 9:
		escribir(a1);
	fin para;  
    
    
  fin.
