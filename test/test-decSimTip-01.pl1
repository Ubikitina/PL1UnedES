# Ejemplo declaracion de constantes y variables de tipo primitivo

programa test:
	constantes
		A = 10;
		B = 20;
		OPEN = cierto;
		CLOSE = falso;
		# open = 1; # debe de emitirse un error semantico por simbolo ya declarado
		
	variables
		abierto, cerrado : booleano;
		x, z : entero;
		# a : booleano; # debe de emitirse un error semantico por simbolo ya declarado
		# ABIERTO : entero; # debe de emitirse un error semantico por simbolo ya declarado

comienzo
	escribir("hola");
	escribir();			
	escribir("mundo");
fin.
