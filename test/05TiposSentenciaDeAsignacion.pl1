programa ejemplo:
	constantes
		MAX = 18;
		FALSE = falso;
		TRUE = cierto;
	tipos
		Mivector = vector [1..5] de entero;
	variables
		abierto, cerrado : booleano;
		x, z: entero;
		v1, v2 : Mivector;
comienzo
	# las siguientes sentencias de asignación son compatibles en cuanto a tipos.
	x = 16;
	x = MAX;
	x = z; # Aqui me da un nullpointer exception
	x = v1[3];
	abierto = TRUE;
	cerrado = abierto;
	x = v2[x];
	v1[2] = x;
	z = x * 5;
	z = (x+5);
	abierto = 5 == x;
	
	# las siguientes sentencias de asignación no son compatibles en cuanto a tipos
	# y debe de emitirse un error semántico.
	z = cierto;
	z = abierto;
	x = v1;
	abierto = v1[3];
	v1 = x;
	v1 = v2;
fin.